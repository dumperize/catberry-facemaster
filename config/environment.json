{
  "title": "FaceMaster.ru",
  "logger": {
    "appenders": [
      {
        "type": "console",
        "category": "catberry"
      },
      {
        "type": "logLevelFilter",
        "level": "ERROR",
        "appender": {
          "type": "file",
          "filename": "erros.log",
          "maxLogSize": 104857600,
          "backups": 10,
          "pollInterval": 15
        }
      },
      {
        "type": "logLevelFilter",
        "level": "TRACE",
        "appender": {
          "type": "file",
          "filename": "trace.log",
          "maxLogSize": 104857600,
          "backups": 10,
          "pollInterval": 15
        }
      }
    ],
    "levels": {
      "catberry": "TRACE"
    }
  },
  "server": {
    "port": 3000
  },
  "api": "http://api.facemaster.ru"
}
