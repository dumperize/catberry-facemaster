<div block="page" mix="block:container">
    <div elem="row">
        <cat-loader id="pages-content-loader"></cat-loader>

        {{#is current '!=' 'main'}}
            {{#is current '!=' 'master-print-card'}}
                <cat-common-breadcrumps id="breadcrumps" cat-store="Breadcrumps"></cat-common-breadcrumps>
            {{/is}}
        {{/is}}

        <div block="content">
            <cat-page-{{current}} id="page-current-{{current}}" cat-store="{{currentStore}}"></cat-page-{{current}}>
        </div>
    </div>
</div>
