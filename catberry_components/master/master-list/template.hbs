<div block="master-list">
    {{#if isHaveMaster}}
        {{#each list}}
            <div elem="item">
                <cat-master-minicard id="master-minicard-{{id}}" master-id="{{id}}"
                                     cat-store="master/MasterList"></cat-master-minicard>
            </div>
        {{/each}}
        <div block='wait-spinner' id="wait-spinner">
            <div elem="animate">{{icon "spinner"}}</div>
        </div>
    {{else}}
        нет мастеров
    {{/if}}
</div><!--master-list-->