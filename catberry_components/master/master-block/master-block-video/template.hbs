<div block="video-md">

    {{#each videos}}
        <div elem="item-cont">

            <div block="video-cont">
                <a href="http://{{url}}" elem="video-cover">
                    <span elem="img-cont"><img src="{{img}}" alt="{{title}}"></span><br>
                    <span elem="title-prev">{{title}}</span>
                </a>
            </div>

        </div>
    {{/each}}

</div><!-- video-md -->
